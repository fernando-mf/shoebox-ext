{
	"name": "shoebox-ext",
	"displayName": "The SHOEBOX extension",
	"description": "A collection of shoebox vscode utils",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.shoeboxRelativePath",
		"onCommand:extension.shoeboxGreet"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.shoeboxRelativePath",
				"title": "Shoebox: Copy relative path to clipboard"
			},
			{
				"command": "extension.shoeboxGreet",
				"title": "Shoebox: Greet"
			}
		],
		"keybindings": [
			{
				"command": "extension.shoeboxRelativePath",
				"key": "ctrl+shift+0",
				"mac": "cmd+shift+0"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {}
}
